{
  "name": "thermocalcdb-nasa",
  "version": "1.0.0",
  "description": "Thermodynamic calculations using NASA7/9 polynomials (TypeScript port)",
  "homepage": "https://github.com/sinagilassi/ThermoCalcDB-NASA#readme",
  "bugs": {
    "url": "https://github.com/sinagilassi/ThermoCalcDB-NASA/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinagilassi/ThermoCalcDB-NASA.git"
  },
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": "./dist/types/index.d.ts"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "csv-parse": "^5.5.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
